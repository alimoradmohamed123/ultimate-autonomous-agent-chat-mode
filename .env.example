# Environment Configuration for Ultimate Autonomous Agent Chat Mode

# GitHub Copilot Configuration
GITHUB_TOKEN=your_github_token_here
COPILOT_API_KEY=your_copilot_api_key_here

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=json

# Security Configuration
ENCRYPTION_KEY=your_encryption_key_here
JWT_SECRET=your_jwt_secret_here

# Tool Orchestration
MAX_CONCURRENT_TOOLS=5
TOOL_TIMEOUT_MS=30000

# Adaptive Learning
LEARNING_DATA_PATH=./data/learning
MODEL_UPDATE_INTERVAL=3600000

# Enterprise Features
AUDIT_LOGGING=true
COMPLIANCE_MODE=true
RATE_LIMITING=true

# Performance Tuning
CACHE_TTL=300000
MAX_MEMORY_MB=1024